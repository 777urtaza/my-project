@model WebApplication2.Models.GhoomoDbContext
@{
    ViewBag.Title = "Login";
}


<h2>Login</h2>
<form method="post">
    <p class="text-danger">@(ViewBag.msg)</p>

    <input class="text" type="text" name="username" value="" placeholder="Enter Username" />
    <input class="text" type="password" name="password" value="" placeholder="Enter password" />

    <button name="submit" type="submit">Login</button>

</form>

